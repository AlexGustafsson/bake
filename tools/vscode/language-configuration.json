{
  "comments": {
    "lineComment": "//"
  },
  "brackets": [
    [ "(", ")" ],
    [ "[", "]" ],
    [ "{", "}" ]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""]
  ],
  "wordPattern": "[a-zA-Z_]\\w*"
}
