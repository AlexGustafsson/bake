{
  "version": "0.1.0",
  "configurations": [
    {
      "type": "extensionHost",
      "request": "launch",
      "name": "Launch VSCode Plugin",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "outFiles": [
        "${workspaceRoot}/client/out/**/*.js"
      ],
      "preLaunchTask": {
        "type": "npm",
        "script": "compile"
      },
      "env": {
        "NODE_ENV": "dev"
      }
    }
  ]
}
